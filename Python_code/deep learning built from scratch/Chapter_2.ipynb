     "output_type": "stream",
     "text": [
      "0\n",
      "0\n",
      "0\n",
      "1\n"
     ]
    }
   ],
   "source": [
    "print(AND(0, 0))\n",
    "print(AND(1,0))\n",
    "print(AND(0,1))\n",
    "print(AND(1, 1))"
   ]
